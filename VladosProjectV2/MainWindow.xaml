                    <ListView Style="{DynamicResource ListViewStyle2}" x:Name="lstvFIO" BorderBrush="{x:Null}" Background="#FFD9D9D9" Margin="95,114,6,0" ScrollViewer.VerticalScrollBarVisibility="Disabled" ScrollViewer.HorizontalScrollBarVisibility="Disabled" Height="80" VerticalAlignment="Top">
                        <ListViewItem x:Name="listViewItem1"  Height="25">
                            <TextBox x:Name="txtSurname" MinWidth="155" Background="{x:Null}" FontSize="16" Width="{Binding ActualWidth, ElementName=listViewItem1}" HorizontalContentAlignment="Stretch" BorderThickness="0,0,0,1" LostFocus="ClientEdit_Leave" IsEnabled="False"/>
                        </ListViewItem>
                        <ListViewItem x:Name="listViewItem2" Height="25">
                            <TextBox x:Name="txtFirstName" MinWidth="155" Background="{x:Null}" FontSize="16" BorderThickness="0,0,0,1" Width="{Binding ActualWidth, ElementName=listViewItem2, Mode=OneWay}" LostFocus="ClientEdit_Leave" IsEnabled="False"/>
                        </ListViewItem>
                        <ListViewItem x:Name="listViewItem3" Height="25">
                            <TextBox x:Name="txtMiddleName" MinWidth="155" Background="{x:Null}" FontSize="16" BorderThickness="0,0,0,1" Width="{Binding ActualWidth, ElementName=listViewItem3, Mode=OneWay}" LostFocus="ClientEdit_Leave" IsEnabled="False"/>
                        </ListViewItem>
                    </ListView>
                    <Label x:Name="lblClientData" Content="Персональные данные клиента" Margin="35,70,0,468" HorizontalAlignment="Left" Width="223" Grid.ColumnSpan="2"/>
                    <Image HorizontalAlignment="Left" Height="20" Margin="10,73,0,0" VerticalAlignment="Top" Width="20" Source="/ClientData.png"/>
                    <Image HorizontalAlignment="Left" Height="20" Margin="10,267,0,0" VerticalAlignment="Top" Width="20" Source="/ClientNumber.png"/>
                </Grid>
            </TabItem>
            <TabItem Style="{DynamicResource TabItemStyle1}" x:Name="DocTab" Header="Документы">
                <Grid Background="White" Margin="-4,7,4,-7">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="178*"/>
                        <ColumnDefinition Width="155*"/>
                        <ColumnDefinition Width="156*"/>
                        <ColumnDefinition Width="45*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="92*"/>
                        <RowDefinition Height="467*"/>
                    </Grid.RowDefinitions>
                    <ComboBox x:Name="cmbDocSearch" Style="{DynamicResource ComboBoxStyle1}" Margin="15,25,1,0" IsEditable="True" BorderBrush="#FFD9D9D9" BorderThickness="0,0,0,0" VerticalContentAlignment="Center" DropDownClosed="cmbDocSearch_DropDownClosed" FontSize="16" Height="35" VerticalAlignment="Top" KeyDown="cmbDocSearch_KeyDown" DropDownOpened="cmbDocSearch_DropDownOpened"/>
                    <ListView x:Name="lstvScan" Style="{DynamicResource ListViewStyle1}" Background="#FFEEEEEE" Grid.ColumnSpan="4" Margin="10,10,10,10" Grid.Row="1">
                        <ListViewItem Style="{DynamicResource ListViewItemStyle1}" Background="#FFDFDEDB">
                            <CheckBox x:Name="Status_Check" Content=" Документ не действителен" Background="{x:Null}" Checked="Status_Check_Checked"/>
                        </ListViewItem>
                        <ListViewItem Height="419" VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch">
                            <Image x:Name="pbScan"/>
                        </ListViewItem>
                    </ListView>
                    <Button x:Name="btnDocDelete" Grid.Column="3" Content="" HorizontalAlignment="Left" Margin="1,28,0,0" VerticalAlignment="Top" Width="30" Height="30" BorderBrush="{x:Null}" Click="btnDocDelete_Click">
                        <Button.Background>
                            <ImageBrush ImageSource="/btnDeleteDoc_Background.png"/>
                        </Button.Background>
                    </Button>
                    <DatePicker Style="{DynamicResource DatePickerStyle1}" x:Name="txtDate_end" Margin="1,25,1,0" VerticalAlignment="Top" Grid.Column="2" Background="#FFBFBFBF" FontSize="20" BorderBrush="{x:Null}" LostFocus="DocEdit_Leave" IsEnabled="False"/>
                    <DatePicker Style="{DynamicResource DatePickerStyle1}" x:Name="txtDate_start" Margin="1,25,1,0" VerticalAlignment="Top" Grid.Column="1" Background="#FFBFBFBF" FontSize="20" BorderBrush="{x:Null}" LostFocus="DocEdit_Leave" IsEnabled="False"/>
                </Grid>
            </TabItem>
        </TabControl>
        <TextBox x:Name="txtManager" TextWrapping="Wrap" BorderBrush="{x:Null}" Background="{x:Null}" RenderTransformOrigin="0.5,0.5" Margin="2,25,181,0" Grid.Column="3" Height="19" VerticalAlignment="Top" TextAlignment="Right" FontWeight="Black" LostFocus="ClientEdit_Leave" IsEnabled="False"/>
    </Grid>
</Window>
